page{
    background-color: #FCFCFC;
}

button::after {
    border: none !important;
    padding: 0 !important;
    margin: 0 !important;
  }
  
  button {
    margin: 0 !important;
    width: 620rpx !important;
    height: 80rpx !important;
    border-radius: 55rpx !important;
    text-align: center !important;
    background-color: #EFC243 !important;
    color: white !important;
    line-height: 50rpx !important;
  }


.success{
    width: 90%;
    height: 950rpx;
    border-radius: 20rpx;
    background-color: white;
    margin: 20rpx auto;
    display: flex;
    justify-content: flex-start;
    align-items: center;
    flex-direction: column;
    padding-top: 120rpx;
    box-shadow: 0px 10px 20px rgba(209, 209, 209, 0.16);
}

.success-top{
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    color: #6A6A6A;
    font-weight: bold;
    font-size: 30rpx;
}


.success-top>image{
    width: 90rpx;
    height: 90rpx;
    margin-bottom: 20rpx;
}


.success-body{
    margin-top: 50rpx;
    display: flex;
    flex-direction: column;
}

.success-body>.body-box{
    display: flex;
    flex-direction: column;
    font-size: 28rpx;
    color: black;
    font-weight: bold;
    line-height: 50rpx;
    margin-top: 20rpx;
}

.success-body>.body-box>.box{
    display: flex;
    justify-content: start;
    align-items: center;
    font-size: 22rpx;
    color: #6A6A6A;
    font-weight: bold;
}

.success-body>.body-box>.box>image{
    width: 20rpx;
    height: 20rpx;
    margin-right: 20rpx;
}

.success-bottom{
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    margin-top: 180rpx;
}

.success-bottom>.but1{
    width: 620rpx;
    height: 80rpx;
    border-radius: 55rpx;
    text-align: center;
    background-color: #EFC243;
    color: white;
    line-height: 80rpx;
}


.success-bottom>.but2{
    width: 620rpx;
    height: 80rpx;
    border-radius: 55rpx;
    margin-top: 40rpx;
    text-align: center;
    border: 1rpx solid #EFC243;
    color:#EFC243;
    line-height: 80rpx;
}