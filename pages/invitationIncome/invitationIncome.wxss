/*  */
page {
  background-color: #fff;
}

.topImg {
  width: 100%;
  height: 250rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}

/*  */
.rili {
  position: fixed;
  top: 200rpx;
  width: 90%;
  margin-left: 5%;
  z-index: 999;
}


.m-tabBox {
  margin-top: 50rpx;
  background-color: #fff;
  height: 270rpx;
  width: 90%;
  margin-left: 5%;
  box-shadow: 0px 8px 6px rgba(193, 188, 188, 0.16);
  border-radius: 20rpx;
}

.m-tabBox>.topBox {
  font-size: 20rpx;
  display: flex;
  justify-content: start;
  align-items: center;
}

.m-tabBox>.topBox>image {
  width: 38rpx;
  height: 38rpx;
}

.m-tabBox>.box {
  display: flex;
  justify-content: center;
  height: 25%;
  width: 80%;
  margin-left: 10%;
  align-items: center;
}

.m-tabBox>.topBox {
  padding: 30rpx;
}

.m-tabBox>.box>.mid {
  width: 0px;
  height: 53rpx;
  border: 3rpx solid rgba(112, 112, 112, 0.3);
}

.m-tabBox>.box>.left>.top {
  position: relative;
  font-size: 24rpx;
  color: #030303;
  font-weight: bold;
  margin-top: 60rpx;
}

.m-tabBox>.box>.left>.bottom {
  font-size: 55rpx;
  font-weight: 600;
  margin-top: 15rpx;
  color: #EFC243;
}

.m-tabBox>.box>.left {
  text-align: center;
}

/*  */

.bigBox {
  width: 90%;
  display: flex;
  flex-direction: column;
  border-radius: 20rpx;
  overflow: hidden;
  margin: 100rpx auto;
}

.m-listTitle {
  width: 100%;
  height: 86rpx;
  background: #EFC243;
  font-size: 25rpx;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.m-lists {
  height: 86rpx;
  font-size: 25rpx;
  font-weight: bold;
  color: hsla(0, 0%, 0%, 1);
  display: flex;
  justify-content: space-between;
  margin: 0 5% 0 5%;
  align-items: center;
  border-bottom: 1px solid rgba(204, 194, 194, 0.4);
}

.m-lists>.list3 {
  width: 180rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}