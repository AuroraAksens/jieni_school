
page{
  background-color: #F8F8F8;
}

/* head */
.money-head{
  width: 100%;
  position: relative;
}
.money-head>.img{
  width: 620rpx;
  height: 323rpx;
  margin: auto;
}
.money-head>.img>image{
  width: 100%;
  height: 100%;
}
.money-head>.text{
  width: 100%;
  height: 323rpx;
  position: absolute;
  top: 0;
}
.money-head>.text>.box{
  width: 620rpx;
  height: 323rpx;
  margin: auto;
}
.money-head>.text>.box>.head{
  font-size: 30rpx;
  font-weight: 500;
}
.money-head>.text>.box>.foot{
  width: 100%;
  height: 140rpx;
}
.money-head>.text>.box>.foot>.left{
  color: #DC5151;
  font-size: 23rpx;
  line-height: 120rpx;
}
.money-head>.text>.box>.foot>.right{
  width: 90%;
  margin: auto;
  font-size: 51rpx;
}
.money-head>.text>.box>.foot>.right>.buttom{
  width: 120rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  color: #FFFFFF;
  background-color: black;
  font-size: 26rpx;
  border-radius: 40rpx;
}
.money-head>.list{
  width: 100%;
}

.body{
  width: 620rpx;
  margin: 30rpx auto;
}
.body>.moneyBox{
  width: 295rpx;
  height: 140rpx;
  background-color: white;
  font-size: 26rpx;
}

.body>.moneyBox>image{
  width: 80rpx;
  height: 80rpx;
}
































